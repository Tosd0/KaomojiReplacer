{
  "name": "emoticon-replacer",
  "version": "1.0.1",
  "description": "基于 BM25 算法的颜文字替换插件",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./core": {
      "require": "./src/core/EmoticonReplacer.js"
    },
    "./data-manager": {
      "require": "./src/core/EmoticonDataManager.js"
    },
    "./search-engine": {
      "require": "./src/core/SearchEngine.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "node examples/test.js",
    "build": "rollup -c",
    "build:watch": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.6"
  },
  "keywords": [
    "emoticon",
    "emoji",
    "text-replacement",
    "bm25",
    "search",
    "sillytavern",
    "api",
    "library",
    "text-processing"
  ],
  "author": "Tosd0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tosd0/EmoticonReplacer"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "src/",
    "data/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "jsdelivr": "dist/emoticon-replacer.umd.min.js",
  "unpkg": "dist/emoticon-replacer.umd.min.js"
}
